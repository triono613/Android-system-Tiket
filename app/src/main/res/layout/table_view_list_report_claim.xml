<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <LinearLayout
        android:layout_gravity="center_horizontal"
        android:background="@drawable/cardview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/txt_nama_lokasi_wisata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:background="@drawable/table_content_cell_bg"
            android:text="nama_lokasi_wisata"
            android:textSize="10sp" />

        <TextView
            android:layout_gravity="top"
            android:id="@+id/txt_nama_lokasi_pintu"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="nama_lokasi_pintu "
            android:textSize="10sp" />

        <TextView
            android:id="@+id/txt_status_karcis"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="status_karcis"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/txt_nama_karcis"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="nama_karcis"
            android:textAlignment="center"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/txt_jumlah_transaksi"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="umlah_transaksi"
            android:textAlignment="center"
            android:textSize="10sp" />


        <TextView
            android:id="@+id/txt_jumlah_wisnu"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="jumlah_wisnu"
            android:textAlignment="center"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/txt_jumlah_tambahan"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="jumlah_tambahan"
            android:textAlignment="center"
            android:textSize="10sp" />


        <TextView
            android:id="@+id/txt_tagihan_wisata"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="tagihan_wisata"
            android:textAlignment="center"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/txt_tagihan_asuransi"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@drawable/table_content_cell_bg"
            android:text="tagihan_asuransi"
            android:textAlignment="center"
            android:textSize="10sp" />


    </LinearLayout>
</LinearLayout>